{
  "manifest_version": 2,

  "name": "Engage-VAMR",
  "description": "Control Google Meets With Alexa",
  "version": "1.0",
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "page_action": {
    "default_icon": "vamr-off.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "*://*/*"
  ]
}
